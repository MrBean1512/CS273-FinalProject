#include <iostream>
#include <fstream>
#include <string>

int main()
{
	std::ifstream myfile("Residents.txt");

	if (!myfile)
	{
		std::cout << "Uh oh, this file could not be opened! Try again! " << std::endl;
		exit(1);
	}
	while (myfile)
	{
		std::string sinput;
		myfile >> sinput;
		std::cout << sinput << std::endl;
	}
	return 0;
}
